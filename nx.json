{
	"$schema": "./node_modules/nx/schemas/nx-schema.json",
	"targetDefaults": {
		"build": {
			"dependsOn": ["^build"],
			"outputs": ["{projectRoot}/dist"],
			"cache": true
		},
		"typecheck": {
			"dependsOn": ["^typecheck"]
		},
		"@nx/js:tsc": {
			"cache": true,
			"dependsOn": ["^build"],
			"inputs": ["default", "^default"]
		},
		"@nx/esbuild:esbuild": {
			"cache": true,
			"dependsOn": ["^build"],
			"inputs": ["default", "^default"]
		}
	},
	"defaultBase": "main",
	"nxCloudAccessToken": "NmZhZGM2MTAtOGRkYS00NzhlLWFlODYtMDQ2ZTc4OTUwODQyfHJlYWQtd3JpdGU=",
	"plugins": [
		{
			"plugin": "@nx/eslint/plugin",
			"options": {
				"targetName": "lint"
			}
		},
		{
			"plugin": "@nx/jest/plugin",
			"options": {
				"targetName": "test"
			}
		}
	]
}
